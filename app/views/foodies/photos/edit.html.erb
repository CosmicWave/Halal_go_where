<h1>Foodies::Photos - Edit</h1>


<%= form_for @photo, url: foodies_photo_path(id: @photo.id), method: :put do |f| %>

			<div class="">
			<%= f.label  :title %></br>
			  <%= f.text_field :title %>
			</div>

			<div class="">
			<%= f.label  :description %></br>
			 <%= f.text_field :description %>
			 </div>
			

			 <div class="">
			<%= f.label  :price %></br></div>
			<div class="">
			<%= f.text_field :price %>
			</div>




			<div class="">
			<%= f.label  :avatar %></br></div>
			<div class="">
			<%= f.file_field :avatar %>
			</div>                                    


			


			<div class="">
	  		<%= f.submit "Submit" %>
	  		</div>
	 
