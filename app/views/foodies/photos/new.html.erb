<h1>Photos - New</h1>


<h3 class="">From for Foodies - photosss</h3>

	<%= form_for :photo, url: foodies_photos_path, method: :post do |f| %>

			<div class="">
			<%= f.label  :title %></br>
			  <%= f.text_field :title %>
			</div>

			<div class="">
			<%= f.label  :description %></br>
			 <%= f.text_field :description %>
			 </div>
			

			 <div class="">
			<%= f.label  :price %></br></div>
			<div class="">
			<%= f.text_field :price %>
			</div>




			<div class="">
			<%= f.label  :avatar %></br></div>
			<div class="">
			<%= f.file_field :avatar %>
			</div>                                    


			


			<div class="">
	  		<%= f.submit "Create" %>
	  		</div>
	 
	 <div class="form">
	 <%= button_tag(type: 'submit', class: "btn btn-success") do %>
	 <%= link_to "Submit", root_path %>
	 <% end %>
	 </div>

<% end %>