<div class="container">
	
	<%= form_for @foody, url: foody_path(@foody), method: :patch do |f| %>

		<%= f.label :name %>
		<%= f.text_field :name %>

		<%= f.label :status %>
		<%= f.text_field :status %>		

		<%= f.label :Your_picture %>
		<%= f.file_field :avatar %>

		<%= f.submit 'Save Changes' %>

	<% end %>

</div>